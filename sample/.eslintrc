{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "new-cap": ["warn", {
      "newIsCap": true,
      "capIsNewExceptions": ["L"]
    }],
    "indent": ["error", 2, {
      "MemberExpression": 1,
      "SwitchCase": 1
    }],
    "quotes": ["error", "single"],
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"],
    "no-empty": 0,
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "spaced-comment": 0,
    "consistent-return": 0,
    "comma-dangle": 0,  // not sure why airbnb turned this on. gross!
    "default-case": 0,
    "func-names": 0,
    "global-require": 0,
    "max-len": 0,
    "no-alert": 0,
    "no-console": ["warn", {
      "allow": ["warn", "error", "log"]
    }],
    "no-native-reassign": ["error", {
      "exceptions": ["$"]
    }],
    "no-extend-native": 0,
    "no-tabs": 0,
    "no-unused-vars": ["warn", {
      "vars": "all",
      "args": "none",
      "ignoreRestSiblings": false
    }],
    "no-param-reassign": warn,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 0,
    "no-use-before-define": ["error", {
      "functions": false,
      "classes": true
    }],
    "no-plusplus": 0,
    "padded-blocks": 0,
    "prefer-template": 0,
    "prefer-arrow-callback": 0,
    "vars-on-top": warn,
    "wrap-iife": 0,
    "arrow-body-style": ["warn", "as-needed"],
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0
  },
  "plugins": [
  ],
  "settings": {
  },
  "globals": {
    "Ti": false,
    "Titanium": false,
    "Alloy": false,
    "$": false,
    "Backbone": false,
    "OS_IOS": false,
    "OS_ANDROID": false,
    "_": false,
    "ENV_DEV": false,
    "ENV_TEST": false,
    "ENV_PRODUCTION": false,
    "ENV_PROD": false,
    "DIST_ADHOC": false,
    "DIST_STORE": false,
    "L": false,
    "alert": false,
    "console": false,
    "Widget": false,
    "WPATH": false,
    "App": false,
    "Parse": true
  }
}
